@prefix ea: <http://www.w3.org/ns/ea#> .
@prefix oa: <http://www.w3.org/ns/oa#> .
@prefix as: <http://www.w3.org/ns/activitystreams#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rfc: <https:/tools.ietf.org/rfc/> .
@prefix tr: <https://www.w3.org/TR/> .
@prefix wcig: <https://wicg.github.io/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .

# Ontology definition
ea: a owl:Ontology ;
    dc:title """EPUB Annotations Vocabulary"""@en ;
    dc:description """RDFS [[rdf-schema]] vocabulary formalizing the terms defined in the [[[epub-ann]]] specification. The vocabulary is a "profile" of the W3C [[[annotation-vocab]]]"""^^rdf:HTML ;
    rdfs:seeAlso <https://www.w3.org/TR/epub-ann-vocab/> ;
    dc:date "2026-02-05"^^xsd:date ;
.

# Property definitions
ea:about a rdf:Property, owl:ObjectProperty ;
    rdfs:domain ea:AnnotationSet ;
    rdfs:range ea:About ;
    rdfs:label "Information on the EPUB publication" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-about>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:conformsTo a rdf:Property, owl:ObjectProperty ;
    rdfs:subPropertyOf dcterms:conformsTo ;
    rdfs:domain ea:Selector ;
    rdfs:range ea:AcceptableFragmentStandards ;
    rdfs:label "Conforms to" ;
    rdfs:comment """<div>The range of acceptable media is restricted for EPUB</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-conformsto>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:hasBody a rdf:Property, owl:ObjectProperty ;
    rdfs:subPropertyOf oa:hasBody ;
    rdfs:domain ea:Annotation ;
    rdfs:range ea:Body ;
    rdfs:label "The body of an annotation" ;
    rdfs:comment """<div>The context file "shadows" the <code>body</code> term in the Web Annotation context file, restricting its range.</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-body>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:color a rdf:Property ;
    rdfs:domain ea:Body ;
    rdfs:range [ a owl:Class; owl:oneOf (ea:pink ea:orange ea:yellow ea:green ea:blue ea:purple) ] ;
    rdfs:label "Color" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-color>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:creator a rdf:Property, owl:ObjectProperty ;
    rdfs:subPropertyOf dcterms:creator ;
    rdfs:range ea:Creator ;
    rdfs:label "The creator of an annotation" ;
    rdfs:comment """<div>The usage of the Dublin Core's <code>dcterm</code> value is restricted to certain type of values...</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-creator>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:generator a rdf:Property, owl:ObjectProperty ;
    rdfs:domain ea:AnnotationSet ;
    rdfs:range ea:Generator ;
    rdfs:label "Generator" ;
    rdfs:comment """<div>The context file "shadows" the <code>generator</code> term in the Web Annotation context file, restricting its range; see formal definition for the restrictions.</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-generator>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:highlight a rdf:Property ;
    rdfs:domain ea:Body ;
    rdfs:range [ a owl:Class; owl:oneOf (ea:solid ea:underline ea:strikethrough ea:outline) ] ;
    rdfs:label "Highlight" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-highlight>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:items a rdf:Property, owl:ObjectProperty ;
    rdfs:domain ea:AnnotationSet ;
    rdfs:range ea:Annotation ;
    rdfs:label "The reference to the set of annotations" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-items>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:meta a rdf:Property, owl:ObjectProperty ;
    rdfs:domain ea:Target ;
    rdfs:range ea:Meta ;
    rdfs:label "Meta" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-meta>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:motivatedBy a rdf:Property ;
    rdfs:subPropertyOf oa:motivatedBy ;
    rdfs:domain ea:Annotation ;
    rdfs:range ea:Motivation, [ a owl:Class; owl:oneOf (oa:bookmarking oa:commenting oa:highlighting) ] ;
    rdfs:label "The motivation for an annotation" ;
    rdfs:comment """<div>The context file "shadows" the <code>body</code> term in the Web Annotation context file, restricting its range.</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-motivation>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:name a rdf:Property, owl:DatatypeProperty ;
    rdfs:subPropertyOf foaf:name ;
    rdfs:domain [ a owl:Class; owl:unionOf (ea:Creator ea:Generator) ] ;
    rdfs:range xsd:string ;
    rdfs:label "Name" ;
    rdfs:comment """<div>This property restricts the usage of <code>foaf:name</code> to be used on creators.</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann-vocab/dfn-name>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:hasSelector a rdf:Property, owl:ObjectProperty ;
    rdfs:subPropertyOf oa:hasSelector ;
    rdfs:range ea:Selector ;
    rdfs:label "Assign selector(s) to a target" ;
    rdfs:comment """<div>The range of this subproperty represents a subset of the Selectors, as defined by the Open Annotation specification.</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-selector>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:hasSource a rdf:Property, owl:ObjectProperty ;
    rdfs:subPropertyOf oa:hasSource ;
    rdfs:domain ea:Target ;
    rdfs:label "Has source" ;
    rdfs:comment """<div>The context file "shadows" the <code>source</code> term in the Web Annotation context file, restricting its domain and controlling its alias name.</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-source>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:tags a rdf:Property, owl:DatatypeProperty ;
    rdfs:domain ea:Body ;
    rdfs:range xsd:string ;
    rdfs:label "Tags" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-tags>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:hasTarget a rdf:Property, owl:ObjectProperty ;
    rdfs:subPropertyOf oa:hasTarget ;
    rdfs:domain ea:Annotation ;
    rdfs:range ea:Target ;
    rdfs:label "The target of an annotation" ;
    rdfs:comment """<div>The context file "shadows" the <code>target</code> term in the Web Annotation context file, restricting its range.</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-target>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:hasTitle a rdf:Property, owl:DatatypeProperty ;
    rdfs:domain ea:AnnotationSet ;
    rdfs:range xsd:string ;
    rdfs:label "Title of the Annotation Set" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-title>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

# Class definitions
ea:About a rdfs:Class ;
    rdfs:label "Information about the EPUB publication" ;
    rdfs:comment """<div>(Externally defined) Dublin Core terms are used with this class, retaining the prefixes (to be in line with the EPUB Package Document)</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-about-object>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:Annotation a rdfs:Class ;
    rdfs:subClassOf oa:Annotation ;
    rdfs:label "EPUB Annotation Object" ;
    rdfs:comment """<div>The Annotation object of EPUB Annotation; the list of acceptable properties are restricted compared to the Web Annotation version.</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-annotation-object>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:AnnotationSet a rdfs:Class ;
    rdfs:label "Set of annotations" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-annotationset>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:Body a rdfs:Class ;
    rdfs:subClassOf oa:TextualBody ;
    rdfs:label "Body of an EPUB Annotation" ;
    rdfs:comment """<div>In EPUB, annotations are restricted to textual content.</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-body-object>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:Creator a rdfs:Class ;
    rdfs:subClassOf [ a owl:Class; owl:unionOf (foaf:Person foaf:Organization as:Application) ] ;
    rdfs:label "Creator of an EPUB Annotation" ;
    rdfs:comment """<div>The context file contains aliases for the possible creator values, JSON-LD serializations should use the terms <code>Person</code>, <code>Organization</code>, or <code>Software</code>.</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-creator-object>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:FragmentSelector a rdfs:Class ;
    rdfs:label "Restricted version of the oa:FragmentSelector, that restricts the list of usable media types" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-fragment-selector-object>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:AcceptableFragmentStandards a rdfs:Class, owl:Class ;
    rdfs:subClassOf dcterms:Standard ;
    owl:oneOf (rfc:rfc3236 tr:media-frags/ tr:SVG/ wcig:scroll-to-text-fragment/) ;
    rdfs:label "Acceptable fragment standards" ;
    rdfs:comment """<div>Defines the acceptable values as fragment types in Fragment Selectors</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-fragment-selector>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:Meta a rdfs:Class ;
    rdfs:label "Meta" ;
    rdfs:comment """<div>this class is currently undefined...</div>"""^^rdf:HTML ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-target-object>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:Generator a rdfs:Class ;
    rdfs:label "Information about the tools generating the annotations" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-generator-object>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:Selector a rdfs:Class ;
    rdfs:subClassOf [ a owl:Class; owl:unionOf (ea:FragmentSelector oa:CssSelector oa:TextQuoteSelector oa:TextPositionSelector) ] ;
    rdfs:label "A restrictive subset of selectors that are usable in EPUB" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#1-3-2-selector>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:Target a rdfs:Class ;
    rdfs:label "Target of an EPUB Annotation" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-target-object>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.



# Definitions of individuals
ea:pink a rdfs:Resource ;
    rdfs:label "Pink selection color" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-color>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:orange a rdfs:Resource ;
    rdfs:label "Orange selection color" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-color>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:yellow a rdfs:Resource ;
    rdfs:label "Yellow selection color" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-color>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:green a rdfs:Resource ;
    rdfs:label "Green selection color" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-color>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:blue a rdfs:Resource ;
    rdfs:label "Blue selection color" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-color>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:purple a rdfs:Resource ;
    rdfs:label "Purple selection color" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-color>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:solid a rdfs:Resource ;
    rdfs:label "Solid selection highlight" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-highlight>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:underline a rdfs:Resource ;
    rdfs:label "Underline selection highlight" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-highlight>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:strikethrough a rdfs:Resource ;
    rdfs:label "Strikethrough selection highlight" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-highlight>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

ea:outline a rdfs:Resource ;
    rdfs:label "Outline selection highlight" ;
    rdfs:isDefinedBy <https://www.w3.org/TR/epub-ann/#dfn-highlight>, <http://www.w3.org/ns/ea#> ;
    vs:term_status "stable" ;
.

# Context files and their mentions
<https://www.w3.org/ns/epub-anno.jsonld> a jsonld:Context ;
    schema:mentions
        ea:About,
        ea:AcceptableFragmentStandards,
        ea:Annotation,
        ea:AnnotationSet,
        ea:Body,
        ea:Creator,
        ea:FragmentSelector,
        ea:Generator,
        ea:Meta,
        ea:Selector,
        ea:Target,
        ea:about,
        ea:blue,
        ea:color,
        ea:conformsTo,
        ea:creator,
        ea:generator,
        ea:green,
        ea:hasBody,
        ea:hasSelector,
        ea:hasSource,
        ea:hasTarget,
        ea:hasTitle,
        ea:highlight,
        ea:items,
        ea:meta,
        ea:motivatedBy,
        ea:name,
        ea:orange,
        ea:outline,
        ea:pink,
        ea:purple,
        ea:solid,
        ea:strikethrough,
        ea:tags,
        ea:underline,
        ea:yellow,
        foaf:homepage ;
.

