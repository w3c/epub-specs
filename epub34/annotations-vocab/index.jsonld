        "rdfs:isDefinedBy": "https://www.w3.org/TR/epub-ann-vocab/dfn-name",
        "vs:term_status": "stable",
        "mentioned": [{
            "@id": "https://www.w3.org/ns/epub-anno.jsonld",
            "@type": "jsonld:Context"
        }]
