        "vs:term_status": "stable",
        "mentioned": [{
            "@id": "https://www.w3.org/ns/epub-anno.jsonld",
            "@type": "jsonld:Context"
        }]
    }, {
        "@id": "ea:Selector",
        "@type": "rdfs:Class",
        "rdfs:subClassOf": {
            "@type": "owl:Class",
            "owl:unionOf": ["oa:FragmentSelector", "oa:CssSelector", "oa:TextQuoteSelector"]
        },
        "rdfs:label": "A restrictive subset of selectors, usable in EPUB",
        "rdfs:comment": {
            "@value": "<div>The final list of selectors for EPUB is still to be defined, this is only a placeholder for now.</div>",
            "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML"
        },
        "rdfs:isDefinedBy": "https://www.w3.org/TR/epub-ann/#dfn-target-object",
