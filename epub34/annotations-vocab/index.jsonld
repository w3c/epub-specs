        "@id": "ea:Annotation",
        "@type": "rdfs:Class",
        "rdfs:subClassOf": ["oa:Annotation"],
        "rdfs:label": "EPUB Annotation Object",
        "rdfs:comment": {
            "@value": "<div>The Annotation object of EPUB Annotation; the list of acceptable properties are restricted compared to the Web Annotation version.</div>",
            "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML"
        },
        "rdfs:isDefinedBy": "https://www.w3.org/TR/epub-ann/#dfn-annotation-object",
