    "rdfs_classes": [{
        "@id": "ea:About",
        "@type": "rdfs:Class",
        "rdfs:label": "Information about the EPUB publication",
        "rdfs:comment": {
            "@value": "<div>(Externally defined) Dublin Core terms are used with this class, retaining the prefixes (to be in line with the EPUB Package Document)</div>",
            "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML"
        },
        "rdfs:isDefinedBy": "https://www.w3.org/TR/epub-ann/#dfn-about-object",
