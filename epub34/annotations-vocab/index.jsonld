        "@id": "ea:Body",
        "@type": "rdfs:Class",
        "rdfs:subClassOf": ["oa:TextualBody"],
        "rdfs:label": "Body of an EPUB Annotation",
        "rdfs:comment": {
            "@value": "<div>In EPUB, annotations are restricted to textual content.</div>",
            "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML"
        },
        "rdfs:isDefinedBy": "https://www.w3.org/TR/epub-ann/#dfn-body-object",
        "vs:term_status": "stable",
        "mentioned": [{
            "@id": "https://www.w3.org/ns/epub-anno.jsonld",
            "@type": "jsonld:Context"
        }]
    }, {
        "@id": "ea:Creator",
        "@type": "rdfs:Class",
        "rdfs:subClassOf": {
            "@type": "owl:Class",
            "owl:unionOf": ["foaf:Person", "foaf:Organization", "as:Application"]
        },
        "rdfs:label": "Creator of an EPUB Annotation",
        "rdfs:comment": {
            "@value": "<div>The context file contains aliases for the possible creator values, JSON-LD serializations should use the terms <code>Person</code>, <code>Organization</code>, or <code>Software</code>.</div>",
            "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML"
        },
        "rdfs:isDefinedBy": "https://www.w3.org/TR/epub-ann/#dfn-creator-object",
        "vs:term_status": "stable",
        "mentioned": [{
            "@id": "https://www.w3.org/ns/epub-anno.jsonld",
            "@type": "jsonld:Context"
        }]
    }, {
        "@id": "ea:Meta",
        "@type": "rdfs:Class",
        "rdfs:label": "Meta",
        "rdfs:comment": {
            "@value": "<div>this class is currently undefined...</div>",
            "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML"
        },
