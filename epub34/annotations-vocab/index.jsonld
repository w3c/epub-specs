        "@type": ["rdf:Property", "owl:ObjectProperty"],
        "rdfs:subPropertyOf": ["oa:hasSelector"],
        "rdfs:range": "ea:Selector",
        "rdfs:label": "Assign selector(s) to a target",
        "rdfs:comment": {
            "@value": "<div>The range of this subproperty represents a subset of the Selectors, as defined by the Open Annotation specification.</div>",
